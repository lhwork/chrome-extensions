{
    "name": "My Flickr Photos",
    "version": "1.0",
    "description": "My Baby photo.",
    "background_page": "background.html",
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "browser_action": {
    	"default_title": "Show Photos",
        "default_icon": "icon.png",
        "popup": "popup.html"
    },
	"permissions": [
	    "tabs", "http://*/*", "http://api.flickr.com/"
    ]
}
